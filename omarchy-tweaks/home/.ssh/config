Host *
    ServerAliveInterval 60
    ServerAliveCountMax 1440
    IdentityFile ~/.ssh/id_ed25519
    IdentitiesOnly yes

Host uio
    Hostname login.math.uio.no
    user kholme

Host atalanta
    Hostname atalanta
    ProxyJump uio
    user kholme
    SendEnv TERM
    SetEnv TERM=xterm-256color

Host nam-shub-01
    Hostname nam-shub-01
    ProxyJump uio
    user kholme
    SendEnv TERM
    SetEnv TERM=xterm-256color

Host nam-shub-02
    Hostname nam-shub-02
    ProxyJump uio
    user kholme
    SendEnv TERM
    SetEnv TERM=xterm-256color
    
Host abacus*
    Hostname %h 
    ProxyJump uio
    user kholme
    SendEnv TERM
    SetEnv TERM=xterm-256color

Host freebio* bioint0*
    HostName %h.hpc.uio.no
    User kholme
    ForwardAgent yes
    ProxyJump uio
    SendEnv TERM
    SetEnv TERM=xterm-256color
